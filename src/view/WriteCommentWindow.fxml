<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxControllers.WriteCommentWindow">
   <children>
      <TextArea fx:id="commentText" prefHeight="175.0" prefWidth="400.0" promptText="Comment" wrapText="true" />
      <Button fx:id="saveButton" layoutY="173.0" mnemonicParsing="false" onAction="#commentAction" prefHeight="25.0" prefWidth="200.0" text="Save" />
      <Button fx:id="cancelButton" layoutX="200.0" layoutY="173.0" mnemonicParsing="false" onAction="#cancelComment" prefHeight="25.0" prefWidth="200.0" text="Cancel" />
   </children>
</AnchorPane>
