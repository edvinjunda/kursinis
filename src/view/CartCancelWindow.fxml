<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="70.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxControllers.CartCancelWindow">
   <children>
      <Label alignment="CENTER" prefWidth="350.0" text="Are you sure that you want to cancel cart?" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="cancelButton" layoutX="236.0" layoutY="33.0" mnemonicParsing="false" onAction="#cancelCanceling" prefWidth="100.0" text="Cancel" />
      <Button fx:id="confirmButton" layoutX="14.0" layoutY="33.0" mnemonicParsing="false" onAction="#confirmCanceling" prefWidth="100.0" text="Confirm" />
   </children>
</AnchorPane>
