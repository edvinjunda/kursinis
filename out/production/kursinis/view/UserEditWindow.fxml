<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="90.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxControllers.UserEditWindow">
   <children>
      <TextField fx:id="editLoginF" layoutY="10.0" prefHeight="25.0" prefWidth="150.0" promptText="login" />
      <TextField fx:id="editPhoneNumF" layoutX="180.0" layoutY="10.0" prefHeight="25.0" prefWidth="120.0" promptText="phone number" />
      <TextField fx:id="editNameF" layoutY="36.0" prefHeight="26.0" prefWidth="94.0" promptText="name" />
      <TextField fx:id="editSurnameF" layoutY="62.0" prefHeight="26.0" prefWidth="94.0" promptText="surname" />
      <TextField fx:id="editCompanyTittleF" layoutX="150.0" layoutY="36.0" prefHeight="25.0" prefWidth="150.0" promptText="company title" />
      <TextField fx:id="editAddressF" layoutX="150.0" layoutY="62.0" prefHeight="25.0" prefWidth="150.0" promptText="address" />
      <Button fx:id="saveEditedUser" layoutX="330.0" layoutY="10.0" mnemonicParsing="false" onAction="#updateUser" prefHeight="25.0" prefWidth="60.0" text="Save" />
      <Button fx:id="cancelButton" layoutX="330.0" layoutY="50.0" mnemonicParsing="false" onAction="#cancelEditing" prefHeight="25.0" prefWidth="60.0" text="Cancel" />
      <Label layoutX="150.0" layoutY="10.0" prefHeight="25.0" prefWidth="30.0" text="+370" />
   </children>
</AnchorPane>
